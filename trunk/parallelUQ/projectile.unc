[Problem]
  executor     = SC
  templateFile = 'projectile.inp'
  storeFlux    = 0
  execDir      = '../projectile/'
  inputDir     = '../projectile/'
  input_editor = 'IE_projectile'
  numprocs     = 1
  mesh_factor  = 1
  time_step    = 1e-4
[]

[HDMR]
  level = 1
  anova = 1
[]

[Sampler]
  [./SC]
    indexSet = TD
    expOrd = 4
    quadrule = single
    anis = 1
  [../]
  [./MC]
    maxSamples = 100000
    writeSamples = 1
    loadSamples = 0
  [../]
[]

[Backend]
  makePDF = 0
  outLabel = 'projectile_anis'
  writeOut = 1
[]

[Variables]
  names = 'm r C rho v ang g sy'
  [./m]
    path = 'Problem/m'
    dist = 'uniform'
    args = '0.145 0.0725'
    weight = 5
  [../]
  [./r]
    path = 'Problem/r'
    dist = 'uniform'
    args = '0.0336 0.00336'
    weight = 5
  [../]
  [./C]
    path = 'Problem/C'
    dist = 'uniform'
    args = '0.5 0.5'
    weight = 1
  [../]
  [./rho]
    path = 'Problem/rho'
    dist = 'uniform'
    args = '1.2 0.1'
    weight = 6
  [../]
  [./v]
    path = 'Problem/v'
    dist = 'uniform'
    args = '50 5'
    weight = 7
  [../]
  [./ang]
    path = 'Problem/ang'
    dist = 'uniform'
    args = '45 10'
    weight = 2
  [../]
  [./g]
    path = 'Problem/g'
    dist = 'uniform'
    args = '9.7988 0.0349'
    weight = 12
  [../]
  [./sy]
    path = 'Problem/sy'
    dist = 'uniform'
    args = '1 1'
    weight = 5
  [../]
[]


