[Problem]
  executor       = SC
 # executor       = MC
  templateFile   = '2G_2D.inp'
  storeFlux      = 0
  execDir        = '../diffusion/'
  inputDir       = '../diffusion/'
  input_editor   = 'IE_Diffusion'
  numprocs       = 6
  mesh_factor    = 5
[]

[Sampler]
  [./MC]
    maxSamples = 50000
    writeSamples = 1
    loadSamples = 1
  [../]
  [./SC]
    indexSet = TD
    expOrd = 6000
    quadrule = single
  [../]
[]

[Backend]
  makePDF = 0
  outLabel = 'N1_iso'
  writeOut = 1
[]

[Variables]
  #names = '1xc2 1xf2 2xc2 2xf2 3xc2 3xf2 4xc2 4xf2 5D2 5xc2'
