[Problem]
  executor     = SC
  templateFile = 'simple.inp'
  storeFlux    = 0
  execDir      = '../simple/'
  inputDir     = '../simple/'
  input_editor = 'IE_Double'
  numprocs     = 2
  mesh_factor  = 1
[]

[Sampler]
  [./SC]
    indexSet = HC
    expOrd = 2
    quadrule = single
  [../]
  [./MC]
    maxSamples = 10000
    writeSamples = 1
    loadSamples = 0
  [../]
[]

[Backend]
  makePDF = 1
  PDFsamples=1e4
  outLabel = 'simple'
  writeOut = 1
  [./PDF]
    bins=50
  [../]
[]

[Variables]
  names = 'x y'
  [./x]
    path = 'Problem/x'
    dist = 'uniform'
    args = '3.5 2.5'
  [../]
  [./y]
    path = 'Problem/y'
    dist = 'uniform'
    args = '3.5 2.5'
  [../]
[]


